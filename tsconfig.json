{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "node",
    "noStrictGenericChecks": true,
    "outDir": "./dist/out-tsc",
    "paths": {
      "app/*": [
        "./app/*"
      ],
      "environments/*": [
        "./environments/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "shared/*": [
        "./shared/*"
      ]
    },
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "jasmine"
    ]
  }
}
